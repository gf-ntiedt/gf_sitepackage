{
    "name": "gedankenfolger/gf-sitepackage",
    "type": "typo3-cms-extension",
    "description": "Basic sitepackage based on bootstrap",
    "homepage": "https://www.gedankenfolger.de/",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-rte-ckeditor": "^12.4",
        "typo3/cms-fluid-styled-content": "^12.4",
        "t3/save": "^2.0.0",
        "ichhabrecht/content-defender": "^3.4.1",
        "fluidtypo3/vhs": "^7.0",
        "mask/mask": "^8.3",
        "b13/container": "^2.3.1",
        "wapplersystems/ws-scss": "^12.0.3"
    },
    "autoload": {
        "psr-4": {
            "Gedankenfolger\\GfSitepackage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "gf_sitepackage"
        }
    }
}
